<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.student.wxzhongyao.dao.GoodsDao">

    <select id="findGoodsByClassId"  parameterType="Integer" resultType="com.student.wxzhongyao.entity.Goods">
		select * from goods where class_id = #{class_id};
	</select>


	<select id="findOne"  parameterType="Integer" resultType="com.student.wxzhongyao.entity.Goods">
		select * from goods where goods_id = #{goods_id};
	</select>

</mapper>


